{
  "name": "ant-design-pro",
  "version": "5.0.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "dev": "npm run start:dev",
    "push-rsync": "rsync -avzhe ssh --delete-before dist/ ubuntu@203.162.10.113:/var/www/slink.ptit.edu.vn/",
    "push-rsync-win": "rsync -avzh --chmod=755 --no-perms --delete-before dist/ ubuntu@203.162.10.113:/var/www/slink.ptit.edu.vn/",
    "deploy": "yarn build && yarn push-rsync",
    "deploy-win": "yarn build && yarn push-rsync-win",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp node ./postinstall.js",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "openapi": "umi openapi",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"src/**/*\"",
    "start": "cross-env UMI_ENV=dev umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev umi dev",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=dev umi dev",
    "start:no-ui": "cross-env UMI_UI=none UMI_ENV=dev umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=dev umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=dev umi dev",
    "pretest": "node ./tests/beforeTest",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "serve": "umi-serve",
    "tsc": "tsc --noEmit",
    "prepare": "husky install",
    "sentry:sourcemaps": ""
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "defaults",
    "not IE <= 11",
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions",
    "last 2 Edge versions",
    "not op_mini all"
  ],
  "dependencies": {
    "@ant-design/icons": "~6.0.0",
    "@ant-design/plots": "^2.4.0",
    "@ant-design/pro-layout": "~7.22.5",
    "@monaco-editor/react": "^4.6.0",
    "@react-oauth/google": "^0.12.2",
    "@sentry/react": "^9.27.0",
    "@tinymce/tinymce-react": "^6.2.1",
    "@types/lodash.debounce": "^4.0.6",
    "@types/react-highlight-words": "^0.20.0",
    "@types/styled-components": "^5.1.26",
    "@umijs/bundler-webpack": "^4.4.11",
    "@umijs/route-utils": "^4.0.1",
    "antd": "5.26.0",
    "antd-img-crop": "^4.2.4",
    "apexcharts": "^4.7.0",
    "axios": "^1.9.0",
    "classnames": "^2.2.6",
    "cors": "^2.8.5",
    "fs-extra": "^11.2.0",
    "js-file-download": "^0.4.12",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "moment-timezone": "^0.6.0",
    "oidc-client-ts": "^3.2.1",
    "path": "^0.12.7",
    "pdfmake": "0.2.20",
    "react": "^19.1.0",
    "react-apexcharts": "^1.4.1",
    "react-beautiful-dnd": "^13.1.0",
    "react-big-calendar": "^1.19.2",
    "react-countup": "^6.4.2",
    "react-dev-inspector": "^2.0.1",
    "react-dom": "^19.1.0",
    "react-helmet-async": "^2.0.5",
    "react-highlight-words": "^0.21.0",
    "react-image-file-resizer": "^0.4.8",
    "react-infinite-scroll-component": "^6.1.0",
    "react-oidc-context": "^3.3.0",
    "react-onesignal": "^3.2.3",
    "react-pdf": "^9.2.1",
    "react-recaptcha": "^2.3.10",
    "react-recaptcha-google": "^1.1.1",
    "react-responsive": "^10.0.1",
    "react-router": "^7.6.2",
    "react-sortable-hoc": "^2.0.0",
    "react-split-pane": "^2.0.3",
    "react-to-print": "^3.1.0",
    "recharts": "^2.15.3",
    "socket.io-client": "^4.5.3",
    "styled-components": "^6.1.18",
    "tinymce": "^7.9.1",
    "umi": "^4.4.11",
    "umi-serve": "~1.9.10",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/express": "^5.0.3",
    "@types/history": "^5.0.0",
    "@types/lodash": "^4.14.144",
    "@types/react": "19.1.7",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-big-calendar": "^1.16.2",
    "@types/react-dom": "^19.1.6",
    "@types/react-helmet": "^6.1.0",
    "@types/react-pdf": "^7.0.0",
    "@types/react-recaptcha": "^2.3.4",
    "@umijs/fabric": "^4.0.1",
    "@umijs/openapi": "^1.13.15",
    "@umijs/plugin-blocks": "^2.0.5",
    "@umijs/plugin-esbuild": "^1.0.1",
    "@umijs/plugin-openapi": "^1.2.0",
    "@umijs/preset-ant-design-pro": "^1.2.0",
    "@umijs/preset-dumi": "^1.1.7",
    "@umijs/preset-react": "^2.1.7",
    "@umijs/yorkie": "^2.0.3",
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "eslint": "^9.28.0",
    "express": "^5.1.0",
    "gh-pages": "^6.3.0",
    "husky": "^9.1.7",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^16.1.0",
    "prettier": "^3.5.3",
    "stylelint": "^16.20.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">= 18.x"
  },
  "gitHooks": {
    "commit-msg": "fabric verify-commit"
  }
}
